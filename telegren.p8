function intro()
 local i=0
 sfx(0)
 while(btn()==0)do
  cls()
  spr(13,52,40,2,2)
  spr(31,68,48)
  spr(45,54,57,3,2)
  if(i>10)spr(15,60,40)
  flip()
  i+=1
  if(i>90)break
 end
end
intro()

frame=0

function l1draw()
 print("‹‘:move",0,100,8)
 print("Ž:teleport",50,90,8)
end

level=1
levels={
 {pstartx=0,
  pstarty=112,
  draw=l1draw},
 {pstartx=0,
  pstarty=112},
 {pstartx=0,
  pstarty=112},
 {pstartx=0,
  pstarty=112},
}

p={x=levels[1].pstartx,y=levels[1].pstarty,dx=0,dy=0,w=7,h=7}
gren={active=false,x=0,y=0,dx=0,dy=0,w=2,h=2}
pdir=1

pspeed=2

g=.75
max_g=6

function _draw() 
 cls()
 map(0,0,0,0,16,16)
 map(level*16,0,0,0,16,16)
 spr(9,levels[level].pstartx,levels[level].pstarty)
 
 if pdir>=0 then
  spr(1,p.x,p.y)
 else
  spr(17,p.x,p.y)
 end
 
 if (gren.active) spr(2,gren.x,gren.y)
 
 if (levels[level].draw) levels[level].draw()
 
 -- todo remove: corner dots
 pset(0,0,8)
 pset(127,0,8)
 pset(0,127,8)
 pset(127,127,8)
end

function _update()
 frame+=1
 if btn(0) then
  p.dx=-pspeed
  pdir=-1
 elseif btn(1) then
  p.dx=pspeed
  pdir=1
 else
  p.dx=0
 end
 if btnp(4) and not gren.active then
  gren.active=true
  gren.x=p.x
  gren.y=p.y
  gren.dx=pdir*2
  gren.dy=-8
 end
 apply_gravity(p)
 local c1=move_thing(p)
 if c1 and band(2,p.ycoll)~=0 then
  p.dy=-8
  p.ycoll=0
 elseif c1 and (band(4,p.xcoll)~=0 or band(4,p.ycoll)~=0) then
  advance_level()
  p.xcoll=0
  p.ycoll=0
 end
 if (gren.active) then
  apply_gravity(gren)
  local c=move_thing(gren)
  if c then
   gren.active=false
   if (not (band(8,gren.xcoll)~=0)) and 
      (not (band(8,gren.ycoll)~=0))
   then
    p.x=flr(gren.x/8)*8
    p.y=flr(gren.y/8)*8
   end
  end
  gren.xcoll=0
  gren.ycoll=0
 end
end

function apply_gravity(t)
 t.dy=min(max_g,t.dy+g)
end

-- note to self: doesn't handle
-- object-object collisions...
-- todo: improve screen clipping
-- to account for width/height
function move_thing(t)
 local st_x=flr(t.x)
 local tgt_x=flr(t.x+t.dx)
 tgt_x=max(min(tgt_x,120),0)
 local dx=sgn(tgt_x-st_x)
 local w=0
 if (dx>0) w=t.w
 local st_y=flr(t.y)
 local tgt_y=flr(t.y+t.dy)
 tgt_y=max(min(tgt_y,120),0)
 local dy=sgn(tgt_y-st_y)
 local h=0
 local coll=false
 if (dy>0) h=t.h
 for i=st_x,tgt_x,dx do
  if check_flags(i+w,t.y)==0 and 
     check_flags(i+w,t.y+t.h)==0 
  then
   t.x=i
  else
   t.dx=0
   t.xcoll=bor(
    check_flags(i+w,t.y),
    check_flags(i+w,t.y+t.h)
   )
   coll=true
   break
  end
 end
 for i=st_y,tgt_y,dy do
  if check_flags(t.x,i+h)==0 and 
     check_flags(t.x+t.w,i+h)==0 
  then
   t.y=i
  else
   t.dy=0
   t.ycoll=bor(
    check_flags(t.x,i+h),
    check_flags(t.x+t.w,i+h)
   )
   coll=true
   break
  end
 end
 return coll
end

function check_flags(x,y)
 return fget(
  mget(level*16+flr(x/8),flr(y/8))
 )
end

function advance_level()
 level+=1
 p.dx=0
 p.dy=0
 p.x=levels[level].pstartx
 p.y=levels[level].pstarty
end

