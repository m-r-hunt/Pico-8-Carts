function draw_gameover()
	draw_playing()
	rectfill(26,32,102,96,12)
	rectfill(27,33,101,95,0)
	print("game over :(",42,40,7)
	print("score:"..tostr(score),30,60,7)
	if new_hs then
		col+=1
		print("new high score!!!",30,70,col)
	else
		print("high score:"..tostr(high_score),30,70,7)
	end
	print("❎/🅾️:back to menu",29,90,7)
end

function update_gameover()
	if btnp(4) or btnp(5) then
		_update=update_menu
		_draw=draw_menu
		init_menu()
		sfx(4)
	end
	update_particles()
end

function init_gameover()
	music(-1)
	new_hs=false
	if score>high_score then
		high_score=score
		dset(0,high_score)
		new_hs=true
		col=0
	end
end

