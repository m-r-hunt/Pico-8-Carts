	[3]={[-2]=true,[-1]=true,[0]=true},
}

function explode(x,y,c,force)
	for a=0,1,0.01 do
		local aa=a+rnd(0.01)-0.005
		add(particles,{x,y,rnd(force)*cos(aa),rnd(force)*sin(aa),c})
	end
end

function gem_ok()
	for x=gx,gx+7 do
		for y=gy,gy+7 do
			if pget(x,y)==0 then
				return false
			end
		end
	end
	return true
end

function spawn_gem()
	local tries=0
	repeat
	 tries+=1
		gx=flr(rnd(112))+8
		gy=flr(rnd(104))+8
	until gem_ok() or tries>10
end

function update_playing()
	cls()
	spr(0,0,0,16,16)
	if (btn(0))pa+=0.03
	if (btn(1))pa-=0.03
	px+=cos(pa)
	py+=sin(pa)
	local tx=flr(px)
	local ty=flr(py)
	if tx~=lastpx or ty~=lastpy then
		if not goodc[pget(tx,ty)] or tx<0 or tx>=128 or ty<0 or ty>120 then
			_draw=draw_gameover
			_update=update_gameover
			explode(tx,ty,8,5)
			sfx(2)
			init_gameover()
		end
		pset(lastpx,lastpy,0)
		local grelx=tx-(gx+4)
		local grely=ty-(gy+4)
		if gemc[grelx] and gemc[grelx][grely] then
			score+=10
			sfx(3)
			explode(gx,gy,11,1)
			gems+=1
			if gems>=gem_target then
				fade_to_new_level()
				gem_target+=1
				gems=0
				gx=-8
				gy=-8
			else
				spawn_gem()
			end
		end
		local a=pa
		a+=rnd(0.2)-0.1
		add(particles,{lastpx,lastpy,-cos(a),-sin(a),9})
		lastpx=tx
		lastpy=ty
		sfx(1)
		steps+=1
		if steps>=100 then
			steps=0
			score+=1
		end
	end
	pset(px,py,15)
	memcpy(0x0000,0x6000,0x2000-8*128/2)
	update_particles()
end

function update_particles()
	for _,p in pairs(particles) do
		p[4]+=0.1
		p[1]+=p[3]
		p[2]+=p[4]
		if p[2]>128 then
			del(particles,p)
		end
	end
end

function draw_playing()
	cls()
	spr(0,0,0,16,15)
	print(score,1,121,7)
	local c=score>high_score and 8 or 7
	print("hi:"..tostr(high_score),65,121,c)
	spr(240,gx,gy)
	for i=1,#particles do
		local p=particles[i]
		pset(p[1],p[2],p[5])
	end
	spr(240,32,120)
	print(gems.."/"..gem_target,40,121,7)
end

