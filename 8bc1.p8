function fade_to_new_level()
	_update=update_fade
	_draw=draw_fade
	ft=0
	fx=0
	flipped=false
	fbase=0
	fdir=1
	flhs=true
	if px<64 then
		fbase=128
		fdir=-1
		flhs=false
		fx=128
	end
end

function ease(t,b,c,d)
	t/=d/2
	if (t<1) return c/2*t*t*t+b
	t-=2
	return c/2*(t*t*t+2)+b
end

function update_fade()
	update_particles()
	ft+=1
	if not flipped then
		fx=ease(ft,fbase,fdir*128,30)
		if fx==fbase+fdir*128 then
			flipped=true
			new_level()
			spawn_gem()
			sfx(6)
			ft=0
			fbase=128-fbase
			fdir*=-1
		end
	else
		fx=ease(ft,fbase,fdir*128,45)
		if fx==fbase+fdir*128 then
			_update=update_playing
			_draw=draw_playing
		end
	end
end

function draw_fade()
	draw_playing()
	
	if flhs then
		rectfill(0,0,fx,120,0)
	else
		rectfill(fx,0,128,120,0)
	end
end


