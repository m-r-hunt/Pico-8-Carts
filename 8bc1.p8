function new_level()
	cls(4)
	for i=1,20 do
		spr(241,rnd(120),rnd(112))
	end
	for i=1,20 do
		spr(242,rnd(120),rnd(112))
	end
	pset(px,py,15)
	memcpy(0x0000,0x6000,0x2000-8*128/2)
end

