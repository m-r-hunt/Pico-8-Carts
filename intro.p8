isfx=0
function intro()
	local i=0
	sfx(isfx)
	while(btn()==0)do
		cls()
		spr(ibase+0,52,40,2,2)
		spr(ibase+18,68,48)
		spr(ibase+32,54,57,3,2)
		if(i>10)spr(ibase+2,60,40)
		flip()
		i+=1
		if(i>90)break
	end
	flip()
end
intro()
