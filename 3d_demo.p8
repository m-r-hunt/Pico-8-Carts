pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- 3d tech demo
-- (c) 2016 max hunt (cc by-sa)

--tip = {64, 0, 64}
points = {}
          
scrx = 20
t = 0

function _update()
 t += 1
 a = (t % 60) / 60
 points[1] = {30+20*cos(a), 60*cos(.25+a), -40}
 points[2] = {30+20*cos(a+.25), 60*cos(.25+a+.25), -40}
 points[3] = {30+20*cos(a+.5), 60*cos(.25+a+.5), -40}
 points[4] = {30+20*cos(a+.75), 60*cos(.25+a+.75), -40}
 points[5] = {30, 0, 90}
end

function _draw()
 cls()
	for pt1 in all(points) do
	 for pt2 in all(points) do
	 	x1 = scrx*pt1[2]/pt1[1]+64
	 	y1 = scrx*pt1[3]/pt1[1]+64
	 	x2 = scrx*pt2[2]/pt2[1]+64
	 	y2 = scrx*pt2[3]/pt2[1]+64
	 	line(x1, y1, x2, y2, 7)
	 end
	end
end

function intro()
 local i=0
 sfx(0)
 while (btn() == 0) do
  cls()
  spr(0, 52, 40, 2, 2)
  spr(18, 68, 48)
  spr(32, 54, 57, 3, 2)
  if(i>10) spr(2,60,40)
  flip()
  i+=1
  if(i>90)break
 end
end

intro()

